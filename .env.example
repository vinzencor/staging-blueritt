# Environment Configuration Template
# Copy this file to .env and update the values according to your environment

# Google reCAPTCHA Site Key
# Get this from https://www.google.com/recaptcha/admin
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here

# Encryption Key (32 characters)
# Generate a secure 32-character string for data encryption
VITE_ENCRYPTION_KEY=your_32_character_encryption_key_here

# API Base URL
# Local Development
VITE_API_URL=http://localhost:8000

# Staging Environment (uncomment for staging)
VITE_API_URL=https://staging-api.blueritt.com

# Production Environment (uncomment for production)
# VITE_API_URL=https://api.blueritt.com
